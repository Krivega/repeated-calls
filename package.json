{
  "name": "repeated-calls",
  "version": "2.2.1",
  "description": "Repeated calls to the function",
  "keywords": [
    "promise"
  ],
  "bugs": {
    "url": "https://github.com/Krivega/repeated-calls/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Krivega/repeated-calls.git"
  },
  "license": "MIT",
  "author": {
    "name": "Krivega Dmitriy",
    "email": "mr.krivega@gmail.com",
    "url": "https://krivega.com"
  },
  "type": "module",
  "exports": {
    ".": {
      "require": "./dist/index.cjs",
      "import": "./dist/index.js"
    }
  },
  "main": "./dist/index.cjs",
  "types": "dist/index.d.ts",
  "typesVersions": {
    "*": {
      "dist/index.d.ts": [
        "dist/index.d.ts"
      ],
      "*": [
        "dist/*"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "vite build",
    "lint": "yarn lint:ts && yarn lint:js",
    "lint:js": "eslint src --ext ts,tsx --ignore-path .gitignore --report-unused-disable-directives --max-warnings 0",
    "lint:ts": "tsc",
    "prepare": "husky",
    "prepublishOnly": "yarn build",
    "release": "standard-version",
    "release:major": "standard-version --release-as major",
    "release:pre": "standard-version --prerelease",
    "release:publish": "git push --follow-tags origin master && npm publish",
    "test": "jest --watch"
  },
  "dependencies": {},
  "devDependencies": {
    "@commitlint/cli": "^19.5.0",
    "@commitlint/config-conventional": "^19.5.0",
    "@types/jest": "^29.5.13",
    "@typescript-eslint/eslint-plugin": "^7.13.0",
    "@typescript-eslint/parser": "^7.13.0",
    "eslint": "^8.7.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "18.0.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jest": "^28.8.3",
    "eslint-plugin-prettier": "^5.2.1",
    "husky": "^9.1.6",
    "jest": "29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "lint-staged": "^15.2.10",
    "prettier": "^3.3.3",
    "standard-version": "^9.5.0",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "tsc-files": "^1.1.4",
    "typescript": "^5.6.2",
    "vite": "^5.4.8",
    "vite-plugin-dts": "^4.2.3",
    "vite-tsconfig-paths": "^5.0.1"
  },
  "peerDependencies": {}
}
